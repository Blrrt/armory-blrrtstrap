.align
  vertical-align middle

clearfix()
  &:after
    clear  both
    content ''
    display table

.center
  text-align center

.clearfix
  clearfix()

.expand
  -webkit-box-sizing border-box
  -moz-box-sizing border-box
  -ms-box-sizing border-box
  box-sizing border-box
  width 100%

.inline-block
  display inline-block
  *display inline

.shrink
  display inline-block
  *display inline
  width normal